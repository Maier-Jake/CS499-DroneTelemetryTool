<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="509.0" prefWidth="697.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="dronetelemetrytool.fxml.InputsSelector">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="welcomeText" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Group 7's Drone Telemetry Tool">
      <font>
         <Font size="23.0" />
      </font>
   </Text>
   <BorderPane prefHeight="65.0" prefWidth="657.0" />

    <Button fx:id="selectCSVButton" alignment="BOTTOM_CENTER" contentDisplay="BOTTOM" prefWidth="89.0" text="Select CSV" />
    <Button fx:id="selectVideoButton" alignment="BOTTOM_CENTER" contentDisplay="BOTTOM" prefWidth="89.0" text="Select MP4" />
</VBox>
