<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dronetelemetrytool.fxml.PopupController" fx:id="popup">
    <stylesheets>
        <URL value="@DTT_Style.css" />
    </stylesheets>
    <alignment>CENTER</alignment>

    <Label text="ERROR" styleClass="error" alignment="CENTER" />

    <Label fx:id="errorText" alignment="CENTER_LEFT"  textAlignment="CENTER"> <padding> <Insets left="5" right="5" bottom="10"/> </padding> </Label>

    <Button fx:id="BUTTON_Close"  onAction="#onCloseClick" text="Okay">
        <cancelButton>true</cancelButton>
    </Button>

</VBox>
